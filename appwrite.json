{
    "projectName": "cver menus",
    "databases": [
        {
            "$id": "cver",
            "name": "cver"
        }
    ],
    "collections": [
        {
            "$id": "menu",
            "$permissions": [
                "read(\"any\")",
                "read(\"guests\")",
                "read(\"users\")"
            ],
            "databaseId": "cver",
            "name": "menu",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "dow",
                    "type": "string",
                    "status": "available",
                    "required": true,
                    "array": false,
                    "size": 20,
                    "default": null
                },
                {
                    "key": "date",
                    "type": "datetime",
                    "status": "available",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "description",
                    "type": "string",
                    "status": "available",
                    "required": false,
                    "array": false,
                    "size": 8000,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "search",
                    "type": "fulltext",
                    "status": "available",
                    "attributes": [
                        "description"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "date_unique",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "date"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        }
    ],
    "functions": [
        {
            "$id": "menu_parser",
            "name": "Menu Parser",
            "runtime": "python-3.11",
            "path": "functions/menu_parser",
            "entrypoint": "src/cver.py",
            "commands": "pip install -r requirements.txt",
            "ignore": [
                "__pypackages__"
            ],
            "execute": [],
            "events": [],
            "schedule": "0 23 * * 0",
            "timeout": 300
        },
        {
            "$id": "menu_image",
            "execute": [],
            "name": "Menu Image Generator",
            "enabled": true,
            "logging": true,
            "runtime": "python-3.11",
            "scopes": [
                "documents.read",
                "documents.write",
                "files.read",
                "files.write"
            ],
            "events": [
                "databases.cver.collections.menu.documents.*.create"
            ],
            "schedule": "",
            "timeout": 300,
            "entrypoint": "generate.py",
            "commands": "pip install -r requirements.txt",
            "specification": "s-1vcpu-512mb",
            "path": "functions/menu_image"
        }
    ],
    "projectId": "689e2e4a0004e00d8fa4"
}